{
    "name": "drawing",
    "scripts": {
        "start": "npm run b:build:schemas && concurrently \"npm run f:build\" \"ts-node-dev src/backend/index.ts\"",
        "b:start:prod": "NODE_ENV=production ts-node src/backend/index.ts",
        "b:build:schemas": "shx mkdir -p src/backend/schemas && typescript-json-schema src/shared/Request.ts Request --id Request.json --out src/backend/schemas/Request.json",
        "b:build:prod": "npm run b:build:schemas && webpack --mode production",
        "build:clean": "shx rm -rf dist && shx rm -rf src/backend/schemas",
        "build:prod": "npm run build:clean && npm run f:build:prod && npm run b:build:prod",
        "f:build": "parcel watch --cache-dir .cache/frontend --public-url '.' --out-dir dist/frontend src/frontend/index.html",
        "f:build:prod": "parcel build --cache-dir .cache/frontend --public-url '.' --out-dir dist/frontend --no-source-maps src/frontend/index.html && npx workbox generateSW",
        "lint": "concurrently \"npm run lint:prettier\" \"npm run lint:tslint\"",
        "lint:prettier": "prettier --ignore-path .gitignore --loglevel warn --write \"**/*.{json,md,ts,yml}\"",
        "lint:tslint": "tslint --project . --fix"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4
    },
    "dependencies": {
        "ajv": "^6.10.2",
        "express": "^4.17.1",
        "mongodb": "^3.3.5",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.3",
        "urijs": "^1.19.2",
        "uuid": "^3.3.3",
        "ws": "^7.2.0"
    },
    "devDependencies": {
        "@types/ajv": "^1.0.0",
        "@types/express": "^4.17.2",
        "@types/mongodb": "^3.3.11",
        "@types/node": "^12.12.20",
        "@types/react": "^16.9.15",
        "@types/react-color": "^3.0.1",
        "@types/react-dom": "^16.9.4",
        "@types/react-redux": "^7.1.5",
        "@types/urijs": "^1.19.4",
        "@types/uuid": "^3.4.6",
        "@types/ws": "^6.0.4",
        "@simonmeusel/react-ionicons": "0.1.0",
        "bulma": "^0.8.0",
        "clipboard": "^2.0.4",
        "concurrently": "^5.0.0",
        "parcel-bundler": "^1.12.4",
        "prettier": "^1.19.1",
        "react": "^16.12.0",
        "react-color": "^2.17.3",
        "react-dom": "^16.12.0",
        "react-draggable": "^4.1.0",
        "react-redux": "^7.1.3",
        "redux": "^4.0.4",
        "sass": "^1.23.7",
        "shx": "^0.3.2",
        "ts-loader": "^6.2.1",
        "ts-node-dev": "^1.0.0-pre.44",
        "tslint": "^5.20.1",
        "typesafe-actions": "^5.1.0",
        "typescript-json-schema": "^0.41.0",
        "webpack": "^4.41.3",
        "webpack-cli": "^3.3.10",
        "webpack-node-externals": "^1.7.2",
        "workbox-cli": "^4.3.1"
    }
}
